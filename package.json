    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.14.0",
    "postcss": "^8.3.6",
    "postcss-scss": "^4.0.0",
    "prettier": "2.3.2",
    "sass": "^1.35.2",
    "tailwindcss": "^2.2.6",
    "@tailwindcss/forms": "^0.3.3",
